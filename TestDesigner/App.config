<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <!--MySql 数据库连接信息-->
    <add key="mysql" value="UserId=root;pwd=Wss#2013;Host=192.168.1.248;Port=3306;Database=rn_ecms;Character Set=utf8;Connect Timeout=3600; Max Pool Size=512;"/>
    <add key="dbtype" value="MySql"/>
    <!-- PLC设备ID -->
    <add key="plcId" value="1"/>
