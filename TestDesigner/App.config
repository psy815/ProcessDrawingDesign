    <!-- 热力公司名称 -->
    <add key="companyname" value="合肥市热力公司"/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Package;3rdLib;MyLibs;SubFolder\Sub.dll"/>
    </assemblyBinding>
  </runtime>
